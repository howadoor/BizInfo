}
.CurrentPage
{
	text-align: center;
}
.PagingNavigation td
{
	padding-left: 2px;
	padding-right: 2px;
}
.PagingRecords td
{
	padding-left: 2px;
	padding-right: 2px;
}
.PagingShowingRecord
{
	padding-left: 2px;
	padding-right: 2px;
}
.searching-criteria 
{
