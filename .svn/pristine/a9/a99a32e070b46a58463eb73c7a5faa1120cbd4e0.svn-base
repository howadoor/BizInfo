	margin-right: 16px;
}

.pager
{
	float: right;
	margin: 0;
	padding: 0;
	width:auto;
}

.bizinfo .tools
{
	float: right;
	display: block;
	font-size: 90%;
	margin-top: 0px;
	padding-left: 16px;
	text-align:right;
}

.bizinfo h2
{
    border-bottom: 1px solid #E0E0E0;
